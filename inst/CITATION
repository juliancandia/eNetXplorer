volume  = "20",
pages   = "189",
textVersion =
paste("Julian Candia and John S. Tsang",
      "eNetXplorer: an R package for the quantitative exploration of elastic net families for generalized linear models,","BMC Bioinformatics (2019) 20:189")
)
